{
    "manifest_version": 3,
    "name": "LinkedIn Auto Connector",
    "version": "1.0",
    "description": "Automatically connects with people on LinkedIn",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["https://www.linkedin.com/*"],
        "js": ["content.js"]
      }
    ]
  }
  